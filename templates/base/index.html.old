<body id="top">
<div class="wrapper col1">
  <div id="header">
    <div id="logo">
      <h1><a href="#">Cari Tutor</a></h1>
      <p>Find Malaysian Tutors for Free</p>
    </div>
    <div id="banner"><a href="#"><img src="/sitemedia/images/demo/468x60.gif" alt="" /></a></div>
    <br class="clear" />
  </div>
</div>
<div class="wrapper col2">
      <div id="topbar">
      <ul>
	<form action="#" method="post">
        <li><p class="title">Tutor Location</p>
	  {{ loc_form.as_p }}
        </li>
        <li><p class="title">Tutor Specialty</p>
	  {{ sub_form.as_p }}
        </li>
	</form>
      </ul>
      </div>
      <br>
      <div id="submit">
        <input type="submit" name="go" id="go" value="FIND TUTOR NOW" />
      </div>
      
    <br class="clear" />
</div>

<div class="wrapper col3">
  <div id="gallery">
    <!--<h1>{ Latest Projects }</h1>-->
    <ul>
      {% for pic in pics %}
      <li><a href="#"><img height="80" width="80" src="/sitemedia/images/{{ pic }} " alt="" /></a></li>
      {% endfor %}
    </ul>
    <br class="clear" />
  </div>
</div>
